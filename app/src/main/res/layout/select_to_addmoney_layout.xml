<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_32dp"
        android:layout_marginTop="@dimen/_24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/textView35"
        style="@style/text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_32dp"
        android:layout_marginTop="@dimen/_20dp"
        android:text="@string/select_how_to_add_money"
        android:textColor="@color/keypad_text_clr"
        app:layout_constraintStart_toEndOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_32dp"
        android:layout_marginTop="@dimen/_32dp"
        android:text="@string/current_bank_account_or_cards"
        android:textColor="@color/common_clr"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView20" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="@dimen/_40dp"
        android:layout_height="@dimen/_40dp"
        android:layout_marginStart="@dimen/_32dp"
        android:layout_marginTop="@dimen/_32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView36"
        app:srcCompat="@drawable/bank_round"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/bankName"
        style="@style/text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_32dp"
        android:layout_marginTop="@dimen/_8dp"
        android:text="Standard Chartered Bank "
        android:textColor="@color/keypad_text_clr"
        app:layout_constraintStart_toEndOf="@+id/imageView21"
        app:layout_constraintTop_toTopOf="@+id/imageView21" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/bankName"
        app:srcCompat="@drawable/beneficery_info"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/textView38"
        style="@style/text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_32dp"
        android:layout_marginTop="@dimen/_176dp"
        android:text="@string/other_methods"
        android:textColor="@color/common_clr"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView36" />


    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_24dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/textView38">

        <ImageView
            android:layout_width="@dimen/_40dp"
            android:layout_height="@dimen/_40dp"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="@dimen/_32dp"
            app:srcCompat="@drawable/direct_bank_addmoney"
            tools:ignore="ContentDescription" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                style="@style/text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:text="@string/directly_from_your_bank"
                android:textColor="@color/colorPrimaryDark" />

            <TextView
                android:layout_width="@dimen/_220dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:text="@string/recommended_usually_arrives_instantly_from_leading_banks"
                android:textColor="@color/common_clr"
                app:layout_constraintStart_toEndOf="@+id/imageView23"
                app:layout_constraintTop_toBottomOf="@+id/textView39" />
        </LinearLayout>


        <Button
            android:layout_width="@dimen/_90dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="@color/transparent"
            android:drawableEnd="@drawable/right_arrow"
            android:text="@string/fees_vary"
            android:textColor="@color/common_clr"
            app:layout_constraintTop_toTopOf="@+id/textView39"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="320dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_16dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <ImageView
            android:layout_width="@dimen/_40dp"
            android:layout_height="@dimen/_40dp"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="@dimen/_32dp"
            app:srcCompat="@drawable/visa_master_addmoney"
            tools:ignore="ContentDescription" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                style="@style/text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:text="@string/add_visa_or_mastercard"
                android:textColor="@color/keypad_text_clr"
                app:layout_constraintStart_toEndOf="@+id/imageView23"
                app:layout_constraintTop_toBottomOf="@+id/textView38" />

            <TextView
                android:layout_width="@dimen/_220dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:text="@string/arrives_instantly"
                android:textColor="@color/common_clr"
                app:layout_constraintStart_toEndOf="@+id/imageView23"
                app:layout_constraintTop_toBottomOf="@+id/textView39" />
        </LinearLayout>


        <Button
            android:layout_width="@dimen/_90dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="@color/transparent"
            android:drawableEnd="@drawable/right_arrow"
            android:text="@string/fees_vary"
            android:textColor="@color/common_clr"
            app:layout_constraintTop_toTopOf="@+id/textView39"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="320dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_16dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout8">

        <ImageView
            android:layout_width="@dimen/_40dp"
            android:layout_height="@dimen/_40dp"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="@dimen/_32dp"
            app:srcCompat="@drawable/rocket_addmoney"
            tools:ignore="ContentDescription" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                style="@style/text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:text="@string/rocket"
                android:textColor="@color/keypad_text_clr"
                app:layout_constraintStart_toEndOf="@+id/imageView23"
                app:layout_constraintTop_toBottomOf="@+id/textView38" />

            <TextView
                android:layout_width="@dimen/_220dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:text="@string/arrives_instantly"
                android:textColor="@color/common_clr"
                app:layout_constraintStart_toEndOf="@+id/imageView23"
                app:layout_constraintTop_toBottomOf="@+id/textView39" />
        </LinearLayout>


        <Button
            android:layout_width="@dimen/_90dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="@color/transparent"
            android:drawableEnd="@drawable/right_arrow"
            android:text="@string/fees_vary"
            android:textColor="@color/common_clr"
            app:layout_constraintTop_toTopOf="@+id/textView39"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="320dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_16dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7">

        <ImageView
            android:id="@+id/imageView28"
            android:layout_width="@dimen/_40dp"
            android:layout_height="@dimen/_40dp"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="@dimen/_32dp"
            app:srcCompat="@drawable/nagad_addmoney"
            tools:ignore="ContentDescription" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                style="@style/text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:text="@string/nagad"
                android:textColor="@color/keypad_text_clr"
                app:layout_constraintStart_toEndOf="@+id/imageView23"
                app:layout_constraintTop_toBottomOf="@+id/textView38" />

            <TextView
                android:layout_width="@dimen/_220dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:text="@string/arrives_instantly"
                android:textColor="@color/common_clr"
                app:layout_constraintStart_toEndOf="@+id/imageView23"
                app:layout_constraintTop_toBottomOf="@+id/textView39" />
        </LinearLayout>


        <Button
            android:layout_width="@dimen/_90dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="@color/transparent"
            android:drawableEnd="@drawable/right_arrow"
            android:text="@string/fees_vary"
            android:textColor="@color/common_clr"
            app:layout_constraintTop_toTopOf="@+id/textView39"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="320dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_16dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout10">

        <ImageView
            android:id="@+id/imageView23"
            android:layout_width="@dimen/_40dp"
            android:layout_height="@dimen/_40dp"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="@dimen/_32dp"
            app:srcCompat="@drawable/paywell_addmoney"
            tools:ignore="ContentDescription" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                style="@style/text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:text="@string/from_a_paywell_account"
                android:textColor="@color/keypad_text_clr"
                app:layout_constraintStart_toEndOf="@+id/imageView23"
                app:layout_constraintTop_toBottomOf="@+id/textView38" />

            <TextView
                android:layout_width="@dimen/_220dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:text="@string/arrives_instantly"
                android:textColor="@color/common_clr"
                app:layout_constraintStart_toEndOf="@+id/imageView23"
                app:layout_constraintTop_toBottomOf="@+id/textView39" />
        </LinearLayout>


        <Button
            android:layout_width="@dimen/_90dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="@color/transparent"
            android:drawableEnd="@drawable/right_arrow"
            android:text="@string/fees_vary"
            android:textColor="@color/common_clr"
            app:layout_constraintTop_toTopOf="@+id/textView39"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="320dp" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>