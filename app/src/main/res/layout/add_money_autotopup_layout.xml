<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout11"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/auto_topup_back_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_32dp"
            android:src="@drawable/back"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="ContentDescription,MissingConstraints" />

        <TextView
            android:id="@+id/textView90"
            style="@style/medium_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/auto_top_up"
            android:textColor="@color/keypad_text_clr"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1dp"
            android:layout_marginTop="@dimen/_16dp"
            android:layout_marginBottom="@dimen/_2dp"
            android:background="@color/common_clr"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView90"
            tools:ignore="MissingConstraints" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView91"
        style="@style/medium_text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_24dp"
        android:layout_marginTop="@dimen/_24dp"
        android:text="@string/auto_top_up_is_on"
        android:textColor="@color/keypad_text_clr"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout11" />

    <Switch
        android:id="@+id/switch1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_24dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView91"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView91" />

    <TextView
        android:id="@+id/textView92"
        android:layout_width="@dimen/_200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="@string/your_account_will_be"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView91" />

    <TextView
        android:id="@+id/textView93"
        style="@style/text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_45dp"
        android:text="@string/when_the_balance_falls_bellow"
        android:textColor="@color/keypad_text_clr"
        app:layout_constraintStart_toStartOf="@+id/textView92"
        app:layout_constraintTop_toBottomOf="@+id/textView92" />

    <TextView
        android:id="@+id/textView94"
        style="@style/medium_text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="200"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="@dimen/_16sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView93"
        app:layout_constraintEnd_toStartOf="@+id/imageView54"
        app:layout_constraintTop_toTopOf="@+id/textView93"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView54"
        android:layout_width="@dimen/_10dp"
        android:layout_height="@dimen/_10dp"
        android:layout_marginEnd="@dimen/_24dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView94"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView94"
        app:srcCompat="@drawable/bottom_arrow"
        tools:ignore="ContentDescription,VectorDrawableCompat" />

    <TextView
        android:id="@+id/textView95"
        style="@style/text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_43dp"
        android:text="@string/auto_top_up_with"
        android:textColor="@color/keypad_text_clr"
        android:textSize="@dimen/_15sp"
        app:layout_constraintStart_toStartOf="@+id/textView93"
        app:layout_constraintTop_toBottomOf="@+id/textView93" />


    <LinearLayout
        android:id="@+id/linearLayout12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_40dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/textView95"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView96"
            android:layout_width="@dimen/_74dp"
            android:layout_height="@dimen/_74dp"
            android:layout_marginStart="@dimen/_35dp"
            android:background="@drawable/round_btn_orange"
            android:gravity="center"
            android:text="@string/_1000"
            android:textColor="@color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView95"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView97"
            android:layout_width="@dimen/_74dp"
            android:layout_height="@dimen/_74dp"
            android:layout_marginLeft="@dimen/_34dp"
            android:layout_marginRight="@dimen/_34dp"
            android:background="@drawable/topup_round_bg"
            android:gravity="center"
            android:text="@string/_1000"
            android:textColor="@color/keypad_text_clr"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="306dp" />

        <TextView
            android:id="@+id/textView98"
            android:layout_width="@dimen/_74dp"
            android:layout_height="@dimen/_74dp"
            android:layout_marginEnd="@dimen/_35dp"
            android:background="@drawable/topup_round_bg"
            android:gravity="center"
            android:text="@string/_1000"
            android:textColor="@color/keypad_text_clr"
            app:layout_constraintBottom_toBottomOf="@+id/textView96"
            app:layout_constraintEnd_toStartOf="@+id/textView97"
            app:layout_constraintStart_toEndOf="@+id/textView96"
            app:layout_constraintTop_toTopOf="@+id/textView96"
            tools:ignore="MissingConstraints" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView99"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_24dp"
        android:layout_marginTop="@dimen/_64dp"
        android:text="@string/using"
        android:textColor="@color/keypad_text_clr"
        android:textSize="@dimen/_15sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout12" />

    <TextView
        android:id="@+id/textView100"
        style="@style/text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_24dp"
        android:text="Fee:    0.00"
        android:textColor="@color/common_clr"
        android:textSize="@dimen/_15sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView99"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView99" />


    <androidx.cardview.widget.CardView
        android:layout_marginEnd="@dimen/_20dp"
        android:layout_marginStart="@dimen/_20dp"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100dp"
        app:cardCornerRadius="@dimen/_5dp"
        android:background="@drawable/topup_round_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView99"
        tools:ignore="MissingConstraints">


        <androidx.constraintlayout.widget.ConstraintLayout
            style="@style/text_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView56"
                android:layout_width="@dimen/_45dp"
                android:layout_height="@dimen/_35dp"
                android:layout_marginStart="@dimen/_17dp"
                android:src="@drawable/mastercard_image"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.507"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/textView101"
                style="@style/medium_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_17dp"
                android:text="Prime Bank Mastercard"
                android:textColor="@color/keypad_text_clr"
                android:textSize="@dimen/_15sp"
                app:layout_constraintStart_toEndOf="@+id/imageView56"
                app:layout_constraintTop_toTopOf="@+id/imageView56" />

            <TextView
                android:id="@+id/textView102"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="*7671"
                android:textColor="@color/common_clr"
                app:layout_constraintStart_toStartOf="@+id/textView101"
                app:layout_constraintTop_toBottomOf="@+id/textView101" />

            <ImageView
                android:id="@+id/imageView57"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_19dp"
                android:layout_marginBottom="@dimen/_16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@drawable/bottom_arrow2"
                tools:ignore="ContentDescription,VectorDrawableCompat" />

            <ImageView
                android:id="@+id/imageView59"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_19dp"
                app:layout_constraintBottom_toTopOf="@+id/imageView57"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.81"
                app:srcCompat="@drawable/top_arrow2"
                tools:ignore="ContentDescription,VectorDrawableCompat" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>