<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/textView446"
        style="@style/text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="NAME"
        android:textColor="@color/common_clr"
        android:textSize="@dimen/_12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/legal_first_name_et" />


    <EditText
        android:id="@+id/legal_first_name_et"
        style="@style/edittext_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawableEnd="@drawable/search_commn_clr"
        android:hint="@string/users"
        android:imeOptions="actionNext"
        android:importantForAutofill="no"
        android:paddingTop="@dimen/_15dp"
        app:layout_constraintTop_toBottomOf="@+id/add_an_user"
        tools:ignore="TextFields" />

    <TextView
        android:id="@+id/textView450"
        style="@style/text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="STATUS"
        android:textColor="@color/common_clr"
        android:textSize="@dimen/_12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView446" />

    <TextView
        android:id="@+id/add_an_user"
        style="@style/text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:drawableStart="@drawable/add_view"
        android:drawablePadding="@dimen/_10dp"
        android:text="@string/add_an_user"
        android:textColor="@color/text_link_clr"
        android:textSize="@dimen/_12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout32"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:padding="@dimen/_5dp"
        app:layout_constraintTop_toBottomOf="@+id/textView446">


        <ImageView
            android:id="@+id/imageView180"
            android:layout_width="@dimen/_34dp"
            android:layout_height="@dimen/_34dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/person" />

        <TextView
            android:textColor="@color/keypad_text_clr"
            android:textSize="@dimen/_14sp"
            style="@style/text_style"
            android:id="@+id/textView451"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Rakibul Islam"
            app:layout_constraintBottom_toBottomOf="@+id/imageView180"
            app:layout_constraintStart_toEndOf="@+id/imageView180"
            app:layout_constraintTop_toTopOf="@+id/imageView180" />

        <TextView
            android:textColor="@color/keypad_text_clr"
            android:textSize="@dimen/_14sp"
            style="@style/text_style"
            android:id="@+id/textView452"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Active"
            app:layout_constraintBottom_toBottomOf="@+id/textView451"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView451" />
    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout33"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:padding="@dimen/_5dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout32">


        <ImageView
            android:id="@+id/imageView180"
            android:layout_width="@dimen/_34dp"
            android:layout_height="@dimen/_34dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/person"
            tools:ignore="DuplicateIds" />

        <TextView
            android:id="@+id/textView451"
            style="@style/text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Arifin"
            android:textColor="@color/keypad_text_clr"
            android:textSize="@dimen/_14sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView180"
            app:layout_constraintStart_toEndOf="@+id/imageView180"
            app:layout_constraintTop_toTopOf="@+id/imageView180"
            tools:ignore="DuplicateIds" />

        <TextView
            android:id="@+id/textView452"
            style="@style/text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Active"
            android:textColor="@color/keypad_text_clr"
            android:textSize="@dimen/_14sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView451"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView451"
            tools:ignore="DuplicateIds" />
    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:padding="@dimen/_5dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout33">


        <ImageView
            android:id="@+id/imageView180"
            android:layout_width="@dimen/_34dp"
            android:layout_height="@dimen/_34dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/person"
            tools:ignore="DuplicateIds" />

        <TextView
            android:id="@+id/textView451"
            style="@style/text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Rasel Hossain"
            android:textColor="@color/keypad_text_clr"
            android:textSize="@dimen/_14sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView180"
            app:layout_constraintStart_toEndOf="@+id/imageView180"
            app:layout_constraintTop_toTopOf="@+id/imageView180"
            tools:ignore="DuplicateIds" />

        <TextView
            android:id="@+id/textView452"
            style="@style/text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Active"
            android:textColor="@color/keypad_text_clr"
            android:textSize="@dimen/_14sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView451"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView451"
            tools:ignore="DuplicateIds" />
    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.constraintlayout.widget.ConstraintLayout>