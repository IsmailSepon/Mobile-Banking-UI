<?xml version="1.0" encoding="utf-8"?>

<layout>

    <data>

        <variable
            name="transferPaywellAcXml"
            type="com.cloudwell.paywell.ui.addMoney.viewModel.TransferPaywellAcViewModel" />
    </data>


    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        style="@style/text_style"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:onClick="@{transferPaywellAcXml::transfer_back_btn}"
            android:id="@+id/transfer_back_btn"
            android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cross_btn"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/textView89"
        style="@style/medium_text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/transfer_to_your_paywell_account"
        android:textColor="@color/keypad_text_clr"
        android:textSize="@dimen/_16sp"
        app:layout_constraintBottom_toBottomOf="@+id/transfer_back_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/transfer_back_btn" />

    <TextView
        android:id="@+id/textView103"
        android:layout_width="@dimen/_250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_16dp"
        android:text="@string/for_using_in_your_internet_banking_beneficiary_addition_page"
        android:textAlignment="center"
        android:textColor="@color/common_clr"
        android:textSize="@dimen/_15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView89" />


    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_220dp"
        android:layout_marginTop="@dimen/_80dp"
        android:layout_marginLeft="@dimen/_12dp"
        android:layout_marginRight="@dimen/_12dp"
        android:background="@drawable/topup_round_bg"
        app:cardCornerRadius="@dimen/_5dp"
        app:layout_constraintTop_toBottomOf="@+id/textView103"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="12dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/textView104"
                style="@style/text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10dp"
                android:layout_marginTop="@dimen/_16dp"
                android:text="@string/beneficery"
                android:textColor="@color/common_clr"
                android:textSize="@dimen/_12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView105"
                style="@style/medium_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10dp"
                android:text="Anisul Islam"
                android:textColor="@color/text_link_clr"
                android:textSize="@dimen/_20sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView104" />

            <TextView
                android:id="@+id/textView106"
                style="@style/text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10dp"
                android:layout_marginTop="@dimen/_20dp"
                android:text="@string/beneficiary_account_number"
                android:textSize="@dimen/_12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView105" />

            <TextView
                android:id="@+id/textView107"
                style="@style/medium_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10dp"
                android:text="7709 6002 3737"
                android:textColor="@color/text_link_clr"
                android:textSize="@dimen/_20sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView106" />

            <TextView
                android:id="@+id/textView108"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10dp"
                android:layout_marginTop="@dimen/_40dp"
                android:text="@string/beneficiary_bank_branch"
                android:textColor="@color/recyclerview_title"
                android:textSize="@dimen/_12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView107" />

            <TextView
                android:id="@+id/textView109"
                style="@style/medium_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_8dp"
                android:text="Standard Chartered Bank, Gulshan"
                android:textColor="@color/text_link_clr"
                android:textSize="@dimen/_20sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView108" />

            <ImageView
                android:id="@+id/imageView52"
                android:layout_width="@dimen/_17sp"
                android:layout_height="@dimen/_19dp"
                android:layout_marginEnd="@dimen/_10dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView104"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView104"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/share"
                tools:ignore="ContentDescription" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/imageView53"
        android:layout_width="@dimen/_25dp"
        android:layout_height="@dimen/_25dp"
        android:layout_marginStart="@dimen/_24dp"
        android:layout_marginTop="@dimen/_54dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView3"
        app:srcCompat="@drawable/clk" />

    <TextView
        android:textColor="@color/keypad_text_clr"
        android:id="@+id/textView110"
        style="@style/text_style"
        android:layout_width="@dimen/_240dp"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:layout_marginStart="@dimen/_10dp"
        android:text="@string/npsb_transfers_takes_5_to_10_minutes_to_appear_in_your_paywell_account"
        android:textSize="@dimen/_13sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView53"
        app:layout_constraintStart_toEndOf="@+id/imageView53"
        app:layout_constraintTop_toTopOf="@+id/imageView53" />

    <ImageView
        android:id="@+id/imageView55"
        android:layout_marginTop="@dimen/_53dp"
        android:layout_width="@dimen/_25dp"
        android:layout_height="@dimen/_25dp"
        app:layout_constraintStart_toStartOf="@+id/imageView53"
        app:layout_constraintTop_toBottomOf="@+id/imageView53"
        app:srcCompat="@drawable/clnder"
        tools:ignore="ContentDescription" />

    <TextView
        android:textSize="@dimen/_13sp"
        android:textColor="@color/keypad_text_clr"
        android:gravity="left"
        android:layout_marginStart="@dimen/_10dp"
        android:id="@+id/textView111"
        android:layout_width="@dimen/_260dp"
        android:layout_height="wrap_content"
        android:text="@string/beftn_transfers_between_12_am_and_2_pm_usually_appears_next_day_1st_half_transfers_between_2_01_pm_and_11_59_pm_appears_next_day_2nd_half_in_your_paywell_account"
        app:layout_constraintBottom_toBottomOf="@+id/imageView55"
        app:layout_constraintStart_toEndOf="@+id/imageView55"
        app:layout_constraintTop_toTopOf="@+id/imageView55" />


    <Button
        android:id="@+id/ac_created_btn"
        android:layout_width="@dimen/_250dp"
        android:layout_height="@dimen/_50dp"
        android:layout_marginTop="@dimen/_32dp"
        android:layout_marginBottom="@dimen/_36dp"
        android:background="@drawable/round_btn_visable"
        android:text="@string/done"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="@dimen/_15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView111"
        app:layout_constraintVertical_bias="0.715" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>