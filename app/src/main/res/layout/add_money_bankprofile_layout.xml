<?xml version="1.0" encoding="utf-8"?>

<layout>

    <data>

        <variable
            name="addmoneyProfilexml"
            type="com.cloudwell.paywell.ui.addMoney.viewModel.AddMoneyBankProfileViewModel" />
    </data>


    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/linearLayout11"
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:background="@color/tollbar_bg"
        android:orientation="horizontal"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:onClick="@{addmoneyProfilexml::bank_back_btn}"
        android:id="@+id/bank_back_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_24dp"
        android:layout_marginTop="@dimen/_24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout11"
        app:srcCompat="@drawable/back"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView46"
        android:layout_width="@dimen/_90dp"
        android:layout_height="@dimen/_90dp"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout11"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout11"
        app:srcCompat="@drawable/bp"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/textView34"
        style="@style/medium_text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_14dp"
        android:text="Prime Bank Mastercard *7671"
        android:textColor="@color/keypad_text_clr"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView46" />

    <ImageView
        android:id="@+id/imageView48"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1dp"
        android:layout_marginTop="@dimen/_40dp"
        android:background="@color/tollbar_bg"
        app:layout_constraintTop_toBottomOf="@+id/textView34"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/imageView47"
        android:layout_width="@dimen/_46dp"
        android:layout_height="@dimen/_46dp"
        android:layout_marginStart="@dimen/_100dp"
        android:layout_marginTop="@dimen/_78dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView34"
        app:srcCompat="@drawable/adjust" />

    <TextView
        android:id="@+id/textView78"
        style="@style/text_style"
        android:layout_width="@dimen/_77dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_12dp"
        android:text="@string/adjust_auto_top_up"
        android:textAlignment="center"
        android:textColor="@color/keypad_text_clr"
        android:textSize="@dimen/_15sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView47"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="@+id/imageView47"
        app:layout_constraintTop_toBottomOf="@+id/imageView47" />

    <ImageView
        android:id="@+id/imageView49"
        android:layout_width="@dimen/_46dp"
        android:layout_height="@dimen/_46dp"
        android:layout_marginStart="@dimen/_83dp"
        android:layout_marginEnd="@dimen/_100dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView47"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView47"
        app:layout_constraintTop_toTopOf="@+id/imageView47"
        app:srcCompat="@drawable/delate" />

    <TextView
        android:id="@+id/textView79"
        style="@style/text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_15sp"
        android:text="@string/delete"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintEnd_toEndOf="@+id/imageView49"
        app:layout_constraintStart_toStartOf="@+id/imageView49"
        app:layout_constraintTop_toBottomOf="@+id/imageView49" />

    <ImageView
        android:id="@+id/imageView50"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_15sp"
        android:layout_marginTop="@dimen/_32dp"
        android:background="@color/divider_bg"
        app:layout_constraintTop_toBottomOf="@+id/textView78"
        tools:ignore="ContentDescription,MissingConstraints" />

    <TextView
        style="@style/medium_text_style"
        android:textColor="@color/recyclerview_title"
        android:textSize="@dimen/_15sp"
        android:id="@+id/textView80"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_24dp"
        android:layout_marginTop="@dimen/_30dp"
        android:text="@string/card_details"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView50" />

    <TextView
        style="@style/medium_text_style"
        android:textColor="@color/recyclerview_title"
        android:textSize="@dimen/normal_txt"
        android:id="@+id/textView81"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_18dp"
        android:text="@string/card_number"
        app:layout_constraintStart_toStartOf="@+id/textView80"
        app:layout_constraintTop_toBottomOf="@+id/textView80" />

    <TextView
        android:textSize="@dimen/_15sp"
        android:textColor="@color/keypad_text_clr"
        style="@style/medium_text_style"
        android:id="@+id/textView82"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_20dp"
        android:text="Mastercard *7671"
        app:layout_constraintBottom_toBottomOf="@+id/textView81"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView81"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        style="@style/medium_text_style"
        android:textColor="@color/recyclerview_title"
        android:textSize="@dimen/normal_txt"
        android:id="@+id/textView83"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_18dp"
        android:text="@string/type"
        app:layout_constraintStart_toStartOf="@+id/textView81"
        app:layout_constraintTop_toBottomOf="@+id/textView81" />

    <TextView
        android:textSize="@dimen/_15sp"
        android:textColor="@color/keypad_text_clr"
        style="@style/medium_text_style"
        android:id="@+id/textView84"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_24dp"
        android:text="@string/credit"
        app:layout_constraintBottom_toBottomOf="@+id/textView83"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView83" />

    <TextView
        style="@style/medium_text_style"
        android:textColor="@color/recyclerview_title"
        android:textSize="@dimen/normal_txt"
        android:id="@+id/textView85"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_18dp"
        android:text="@string/valid_until"
        app:layout_constraintStart_toStartOf="@+id/textView83"
        app:layout_constraintTop_toBottomOf="@+id/textView83" />

    <TextView
        android:textSize="@dimen/_15sp"
        android:textColor="@color/keypad_text_clr"
        style="@style/medium_text_style"
        android:id="@+id/textView86"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_24dp"
        android:text="08/23"
        app:layout_constraintBottom_toBottomOf="@+id/textView85"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView85" />

    <TextView
        style="@style/medium_text_style"
        android:textColor="@color/recyclerview_title"
        android:textSize="@dimen/normal_txt"
        android:id="@+id/textView87"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_18dp"
        android:text="@string/bank_name"
        app:layout_constraintStart_toStartOf="@+id/textView85"
        app:layout_constraintTop_toBottomOf="@+id/textView85" />

    <TextView
        android:textSize="@dimen/_15sp"
        android:textColor="@color/keypad_text_clr"
        style="@style/medium_text_style"
        android:id="@+id/textView88"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_24dp"
        android:text="Prime Bank"
        app:layout_constraintBottom_toBottomOf="@+id/textView87"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView87" />

    <TextView
        android:onClick="@{addmoneyProfilexml::bank_profile_edit}"
        android:textSize="15sp"
        android:textColor="@color/colorPrimaryDark"
        style="@style/medium_text_style"
        android:id="@+id/bank_profile_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_18dp"
        android:layout_marginEnd="@dimen/_24dp"
        android:text="@string/edit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView88" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>