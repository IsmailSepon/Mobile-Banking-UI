<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    app:cardElevation="@dimen/_15dp"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/transparent"
    android:layout_width="wrap_content"
    android:paddingBottom="@dimen/_20dp"
    android:layout_height="wrap_content">


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/_20dp"
        app:cardElevation="@dimen/_10dp"
        app:cardCornerRadius="@dimen/_10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout34"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/textView112"
                style="@style/text_style"
                android:layout_width="@dimen/_240dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24dp"
                android:layout_marginTop="@dimen/_25dp"
                android:layout_marginEnd="@dimen/_24dp"
                android:gravity="left"
                android:text="@string/date_and_type_filters_are_applied_all_ntimestamps_are_in_utc_and_your_account_timezone"
                android:textColor="@color/common_clr"
                android:textSize="@dimen/_12sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.15"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:textColor="@color/keypad_text_clr"
                android:textSize="@dimen/_12sp"
                style="@style/text_style"
                android:id="@+id/textView456"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="50dp"
                android:layout_marginTop="12dp"
                android:text="@string/only_completed_transactions"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView112" />


            <ImageView
                android:id="@+id/imageView184"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1dp"
                android:layout_marginTop="20dp"
                android:background="@color/divider_bg"
                app:layout_constraintTop_toBottomOf="@+id/textView456"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/downlodeCsv_txt"
                style="@style/text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@string/download_as_csv"
                android:textColor="@color/keypad_text_clr"
                android:textSize="@dimen/_12sp"
                app:layout_constraintStart_toStartOf="@+id/textView112"
                app:layout_constraintTop_toBottomOf="@+id/imageView184" />

            <TextView
                android:textColor="@color/keypad_text_clr"
                android:textSize="@dimen/_12sp"
                style="@style/text_style"
                android:id="@+id/downlodePdf_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/download_as_pdf"
                app:layout_constraintStart_toStartOf="@+id/downlodeCsv_txt"
                app:layout_constraintTop_toBottomOf="@+id/downlodeCsv_txt" />

            <TextView
                android:id="@+id/another_formate_txt"
                style="@style/text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="20dp"
                android:text="@string/need_another_format"
                android:textColor="@color/keypad_text_clr"
                android:textSize="@dimen/_12sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/downlodePdf_txt"
                app:layout_constraintTop_toBottomOf="@+id/downlodePdf_txt" />

            <TextView
                android:textColor="@color/common_clr"
                android:textSize="@dimen/_12sp"
                style="@style/text_style"
                android:id="@+id/letusKnow_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:text="@string/let_us_know"
                app:layout_constraintBottom_toBottomOf="@+id/another_formate_txt"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/another_formate_txt" />

            <CheckBox
                android:id="@+id/checkBox2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:button="@drawable/checkbox_selector"
                app:layout_constraintBottom_toBottomOf="@+id/textView456"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView456" />


        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.cardview.widget.CardView>

</LinearLayout>