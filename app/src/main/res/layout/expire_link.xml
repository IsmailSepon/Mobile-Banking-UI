<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <Spinner
        android:id="@+id/expair_sp"
        style="@style/SpinnerTheme"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="16dp"
        android:background="@drawable/spinner_bg_solid"
        android:gravity="center"
        android:spinnerMode="dropdown"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/imageView45"
        android:layout_width="@dimen/_0dp"
        android:layout_height="@dimen/_2dp"
        android:background="@color/common_clr"
        app:layout_constraintBottom_toBottomOf="@+id/expair_sp"
        app:layout_constraintEnd_toEndOf="@+id/expair_sp"
        app:layout_constraintStart_toStartOf="@+id/expair_sp"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView229"
        android:layout_width="@dimen/_15dp"
        android:layout_height="@dimen/_15dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView45"
        app:layout_constraintStart_toEndOf="@+id/expair_sp"
        app:layout_constraintTop_toTopOf="@+id/expair_sp"
        app:srcCompat="@drawable/close_expire_sp" />

</androidx.constraintlayout.widget.ConstraintLayout>