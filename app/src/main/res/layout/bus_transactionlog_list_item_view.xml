<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/mainClickLL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/_5dp"
        android:background="@color/hader_bg">


    <TextView
        style="@style/text_style"
        android:textColor="@color/common_clr"
            android:id="@+id/bookingIdStringTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Booking ID:"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="@+id/WebBookingIDTV"
            app:layout_constraintTop_toBottomOf="@+id/webBookingId" />


    <TextView
        android:id="@+id/bookingIdTV"
        style="@style/text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:hint="Id number"
        android:textColor="@color/keypad_text_clr"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.004"
        app:layout_constraintStart_toEndOf="@+id/bookingIdStringTV"
        app:layout_constraintTop_toTopOf="@+id/bookingIdStringTV" />

    <TextView
        style="@style/text_style"
        android:textColor="@color/keypad_text_clr"
            android:id="@+id/tvTransactionId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:hint="Id number"
            android:textSize="14sp"
            app:layout_constraintStart_toEndOf="@+id/textView34"
            app:layout_constraintTop_toTopOf="@+id/textView34"
            tools:text="sdfsdfhsjkhdfkjhsfjkhsdjfkhjksdfh" />

    <TextView
        style="@style/text_style"
        android:textColor="@color/common_clr"
            android:id="@+id/WebBookingIDTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Web Booking ID:"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="@+id/statusStringTV"
            app:layout_constraintTop_toBottomOf="@+id/statusTV" />

    <TextView
        style="@style/text_style"
        android:textColor="@color/keypad_text_clr"
            android:id="@+id/webBookingId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:hint="Id number"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/imageView21"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/WebBookingIDTV"
            app:layout_constraintTop_toTopOf="@+id/WebBookingIDTV" />


    <ImageView
            android:id="@+id/imageView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/priceTV"
            app:layout_constraintEnd_toStartOf="@+id/priceTV"
            app:layout_constraintTop_toTopOf="@+id/priceTV"
            app:srcCompat="@drawable/ic_bus_transtion_log" />

    <TextView
        style="@style/text_style"
        android:id="@+id/priceTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:padding="5dp"
        android:text="8999"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.59000003" />


    <TextView
        style="@style/text_style"
        android:textColor="@color/common_clr"
        android:id="@+id/statusStringTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:text="Status Message:"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        style="@style/text_style"
        android:gravity="left"
        android:textColor="@color/keypad_text_clr"
        android:id="@+id/statusTV"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:hint="booking"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/statusStringTV"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="sdflskdjflksdjfkljsdflkjsdklfjslkdfjklsjflkjslkdfjlksjdfksjdflkjsdlkf" />

    <TextView
        style="@style/text_style"
        android:textColor="@color/common_clr"
            android:id="@+id/textView34"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:text="Transaction ID: "
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/bookingIdStringTV"
            app:layout_constraintTop_toBottomOf="@+id/bookingIdTV" />

    <ImageView
            android:id="@+id/ivCopy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/tvTransactionId"
            app:layout_constraintStart_toEndOf="@+id/tvTransactionId"
            app:layout_constraintTop_toTopOf="@+id/tvTransactionId"
            app:srcCompat="@drawable/copy_icon" />


</androidx.constraintlayout.widget.ConstraintLayout>