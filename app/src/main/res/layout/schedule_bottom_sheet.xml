<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="schedulebottomsheetModel"
            type="com.cloudwell.paywell.consumer.ui.scheduled_Transfer.viewModel.ScheduleBottomSheetViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/schedulebottomsheet_bg"
        android:orientation="vertical">


        <Button
            android:id="@+id/button"
            android:layout_width="250dp"
            android:layout_height="50dp"
            android:layout_marginTop="55dp"
            android:layout_marginBottom="32dp"
            android:background="@drawable/round_btn_orange"
            android:text="@string/set"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout14"
            app:layout_constraintVertical_bias="0.432"
            tools:ignore="MissingConstraints" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout14"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginTop="18dp"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout13">

            <ImageView
                android:id="@+id/imageView69"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="37dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/p_blue"
                tools:ignore="VectorDrawableCompat" />

            <TextView
                android:id="@+id/textView148"
                style="@style/text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:text="@string/repeat"
                android:textColor="@color/keypad_text_clr"
                android:textSize="13sp"
                app:layout_constraintStart_toEndOf="@+id/imageView69"
                tools:ignore="MissingConstraints" />


            <TextView
                android:id="@+id/textView149"
                style="@style/text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/every_day"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="@dimen/eleven_sp"
                app:layout_constraintStart_toStartOf="@+id/textView148"
                app:layout_constraintTop_toBottomOf="@+id/textView148" />


        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/textView138"
            style="@style/medium_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="39dp"
            android:text="What do you want to do"
            android:textColor="@color/keypad_text_clr"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView147"
            style="@style/medium_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="39dp"
            android:layout_marginEnd="34dp"
            android:text="@string/btn_clear"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout13"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/textView138">

            <ImageView
                android:id="@+id/imageView70"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="37dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/mini_calender"
                tools:ignore="VectorDrawableCompat" />

            <TextView
                android:id="@+id/textView14"
                style="@style/text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:text="@string/date"
                android:textColor="@color/keypad_text_clr"
                android:textSize="13sp"
                app:layout_constraintStart_toEndOf="@+id/imageView70"
                tools:ignore="MissingConstraints" />


            <TextView
                android:id="@+id/textView15"
                style="@style/text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1 Apr 2020"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="@dimen/eleven_sp"
                app:layout_constraintStart_toStartOf="@+id/textView14"
                app:layout_constraintTop_toBottomOf="@+id/textView14" />


        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>