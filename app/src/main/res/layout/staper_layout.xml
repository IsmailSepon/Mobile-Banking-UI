<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <View
        android:id="@+id/circle_one"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginStart="40dp"
        android:background="@drawable/round_btn_orange"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.07999998" />

    <View
        android:id="@+id/line_one"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_toEndOf="@+id/circle_one"
        android:background="@color/common_clr"
        app:layout_constraintBottom_toBottomOf="@+id/circle_one"
        app:layout_constraintEnd_toStartOf="@+id/circle_two"
        app:layout_constraintStart_toEndOf="@+id/circle_one"
        app:layout_constraintTop_toTopOf="@+id/circle_one" />

    <View
        android:id="@+id/circle_two"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_toEndOf="@+id/line_one"
        android:background="@drawable/topup_round_bg"
        app:layout_constraintBottom_toBottomOf="@+id/line_one"
        app:layout_constraintEnd_toStartOf="@+id/circle_three"
        app:layout_constraintStart_toEndOf="@+id/circle_one"
        app:layout_constraintTop_toTopOf="@+id/line_one" />

    <View
        android:id="@+id/line_two"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_toEndOf="@+id/circle_two"
        android:background="@color/common_clr"
        app:layout_constraintBottom_toBottomOf="@+id/circle_two"
        app:layout_constraintEnd_toStartOf="@+id/circle_three"
        app:layout_constraintStart_toEndOf="@+id/circle_two"
        app:layout_constraintTop_toTopOf="@+id/circle_two" />

    <View
        android:id="@+id/circle_three"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginEnd="40dp"
        android:layout_toEndOf="@+id/line_two"
        android:background="@drawable/topup_round_bg"
        app:layout_constraintBottom_toBottomOf="@+id/circle_two"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/circle_two" />

    <TextView
        style="@style/text_style"
        android:textSize="@dimen/_14sp"
        android:textColor="@color/staper_bg"
        android:id="@+id/textView285"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Ordered"
        app:layout_constraintEnd_toStartOf="@+id/line_one"
        app:layout_constraintStart_toStartOf="@+id/circle_one"
        app:layout_constraintTop_toBottomOf="@+id/circle_one" />

    <TextView
        style="@style/text_style"
        android:textSize="@dimen/_14sp"
        android:textColor="@color/keypad_text_clr"
        android:id="@+id/textView286"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Posted"
        app:layout_constraintEnd_toEndOf="@+id/circle_two"
        app:layout_constraintStart_toStartOf="@+id/circle_two"
        app:layout_constraintTop_toBottomOf="@+id/circle_two" />

    <TextView
        style="@style/text_style"
        android:textSize="@dimen/_14sp"
        android:textColor="@color/staper_bg"
        android:id="@+id/textView287"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mar 2"
        app:layout_constraintEnd_toEndOf="@+id/textView286"
        app:layout_constraintStart_toStartOf="@+id/textView286"
        app:layout_constraintTop_toBottomOf="@+id/textView286" />

    <TextView
        style="@style/text_style"
        android:textSize="@dimen/_14sp"
        android:textColor="@color/keypad_text_clr"
        android:id="@+id/textView288"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Expected"
        app:layout_constraintEnd_toEndOf="@+id/circle_three"
        app:layout_constraintStart_toEndOf="@+id/line_two"
        app:layout_constraintTop_toBottomOf="@+id/circle_three" />

    <TextView
        style="@style/text_style"
        android:textSize="@dimen/_14sp"
        android:textColor="@color/staper_bg"
        android:id="@+id/textView289"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mar 12"
        app:layout_constraintEnd_toEndOf="@+id/textView288"
        app:layout_constraintStart_toStartOf="@+id/textView288"
        app:layout_constraintTop_toBottomOf="@+id/textView288" />


</androidx.constraintlayout.widget.ConstraintLayout>