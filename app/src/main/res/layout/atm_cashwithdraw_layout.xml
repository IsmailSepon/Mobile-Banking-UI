<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:paddingBottom="@dimen/_80dp"
        android:layout_height="wrap_content">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout9"
            android:layout_width="match_parent"
            android:layout_height="@dimen/fouty_dp"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/atm_withdraw_back"
                android:layout_width="@dimen/_14dp"
                android:layout_height="@dimen/_14dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_20dp"
                app:srcCompat="@drawable/cross_btn"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription,MissingConstraints" />

            <TextView
                style="@style/medium_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/atm_cash_withdrawal_code"
                android:textColor="@color/keypad_text_clr"
                android:textSize="@dimen/_16sp"
                android:theme="@style/medium_text_style"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.538"
                tools:ignore="MissingConstraints" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/textView197"
            android:layout_width="@dimen/_276dp"
            android:layout_marginTop="@dimen/_5dp"
            android:textAlignment="center"
            android:textColor="@color/recyclerview_title"
            android:textSize="@dimen/_15sp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_45dp"
            android:layout_marginStart="@dimen/_45dp"
            android:text="@string/for_using_in_any_of_the_following_banks_atms_with_cash_by_code"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout9" />


        <androidx.cardview.widget.CardView
            android:id="@+id/cardView5"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/round_btn"
            app:cardCornerRadius="@dimen/_5dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView197">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <TextView
                    android:id="@+id/textView199"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="18dp"
                    android:layout_marginTop="12dp"
                    android:text="@string/code"
                    android:textSize="@dimen/_12sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView201"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="18dp"
                    android:layout_marginTop="24dp"
                    android:text="PIN"
                    android:textSize="@dimen/_12sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView200" />

                <ImageView
                    android:id="@+id/imageView98"
                    android:layout_width="@dimen/_17dp"
                    android:layout_height="@dimen/_19dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginEnd="12dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/share" />

                <TextView
                    android:id="@+id/textView200"
                    style="@style/medium_text_style"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="91817161"
                    android:textColor="@color/text_link_clr"
                    android:textSize="@dimen/_20sp"
                    app:layout_constraintStart_toStartOf="@+id/textView199"
                    app:layout_constraintTop_toBottomOf="@+id/textView199" />

                <TextView
                    android:id="@+id/textView202"
                    style="@style/medium_text_style"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="9181"
                    android:textColor="@color/text_link_clr"
                    android:textSize="@dimen/_20sp"
                    app:layout_constraintStart_toStartOf="@+id/textView199"
                    app:layout_constraintTop_toBottomOf="@+id/textView201" />
            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>

        <LinearLayout
            android:id="@+id/linearLayout16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="18dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cardView5">

            <ImageView
                android:id="@+id/imageView99"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cardView5"
                app:srcCompat="@drawable/search_loc_ic"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/textView203"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_12dp"
                android:text="@string/atm_near_me"
                android:textColor="@color/text_link_clr"
                android:textSize="@dimen/_17sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView99"
                app:layout_constraintStart_toEndOf="@+id/imageView99"
                app:layout_constraintTop_toTopOf="@+id/imageView99" />

        </LinearLayout>

        <ImageView
            android:id="@+id/imageView100"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="22dp"
            android:layout_marginTop="52dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout16"
            app:srcCompat="@drawable/atm_ic" />

        <TextView
            android:id="@+id/textView204"
            style="@style/text_style"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="60dp"
            android:layout_marginEnd="32dp"
            android:gravity="left"
            android:lineHeight="@dimen/_17sp"
            android:text="@string/when_you_make_a_withdrawal_the_cash_can_only_be_withdrawn_at_a_the_city_bank_atm_southeast_bank_atm"
            android:textColor="@color/keypad_text_clr"
            android:textSize="@dimen/_14sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView100"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView100" />

        <TextView
            android:id="@+id/textView205"
            style="@style/text_style"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="60dp"
            android:layout_marginEnd="32dp"
            android:gravity="left"
            android:lineHeight="@dimen/_17sp"
            android:text="Withdrawal vouchers are only valid for 24 hours after which they are refunded back to your wallet balance."
            android:textColor="@color/keypad_text_clr"
            android:textSize="@dimen/_14sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView102"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView102" />


        <TextView
            android:id="@+id/textView206"
            style="@style/text_style"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="60dp"
            android:layout_marginEnd="32dp"
            android:gravity="left"
            android:lineHeight="@dimen/_17sp"
            android:text="you can find a record of the ATM withdrawal in your transaction list."
            android:textColor="@color/keypad_text_clr"
            android:textSize="@dimen/_14sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView103"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView103" />


        <ImageView
            android:id="@+id/imageView104"
            android:layout_width="@dimen/_24dp"
            android:layout_height="@dimen/_24dp"
            android:layout_marginTop="@dimen/_40dp"
            app:layout_constraintStart_toStartOf="@+id/imageView103"
            app:layout_constraintTop_toBottomOf="@+id/imageView103"
            app:srcCompat="@drawable/pin_ic" />

        <TextView
            android:id="@+id/textView207"
            style="@style/text_style"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_60dp"
            android:layout_marginEnd="@dimen/_32dp"
            android:gravity="left"
            android:lineHeight="@dimen/_17sp"
            android:text="When you want to collect the cash, enter the PIN given above on the voucher (not your account PIN) on the ATM"
            android:textColor="@color/keypad_text_clr"
            android:textSize="@dimen/_14sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView104"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView104"
            app:layout_constraintVertical_bias="0.473" />


        <ImageView
            android:id="@+id/imageView102"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            app:layout_constraintStart_toStartOf="@+id/imageView100"
            app:layout_constraintTop_toBottomOf="@+id/imageView100"
            app:srcCompat="@drawable/time_ic" />

        <ImageView
            android:id="@+id/imageView103"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            app:layout_constraintStart_toStartOf="@+id/imageView102"
            app:layout_constraintTop_toBottomOf="@+id/imageView102"
            app:srcCompat="@drawable/doc_ic" />


        <Button
            android:id="@+id/cash_withdraw_done_btn"
            android:layout_width="@dimen/btn_width"
            android:layout_height="@dimen/btn_hight"
            android:layout_marginTop="50dp"
            android:background="@drawable/round_btn_visable"
            android:elevation="@dimen/evelation_five"
            android:text="@string/done"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView207"
            tools:targetApi="lollipop" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</ScrollView>